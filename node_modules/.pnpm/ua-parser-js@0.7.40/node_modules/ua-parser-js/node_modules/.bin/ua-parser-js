#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/devsecops/learn/hotel-list/node_modules/.pnpm/ua-parser-js@0.7.40/node_modules/ua-parser-js/script/node_modules:/home/devsecops/learn/hotel-list/node_modules/.pnpm/ua-parser-js@0.7.40/node_modules/ua-parser-js/node_modules:/home/devsecops/learn/hotel-list/node_modules/.pnpm/ua-parser-js@0.7.40/node_modules:/home/devsecops/learn/hotel-list/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/devsecops/learn/hotel-list/node_modules/.pnpm/ua-parser-js@0.7.40/node_modules/ua-parser-js/script/node_modules:/home/devsecops/learn/hotel-list/node_modules/.pnpm/ua-parser-js@0.7.40/node_modules/ua-parser-js/node_modules:/home/devsecops/learn/hotel-list/node_modules/.pnpm/ua-parser-js@0.7.40/node_modules:/home/devsecops/learn/hotel-list/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../script/cli.js" "$@"
else
  exec node  "$basedir/../../script/cli.js" "$@"
fi
